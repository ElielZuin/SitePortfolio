<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <urlCompression doDynamicCompression="false" />
        <defaultDocument enabled="true">
            <files>
                <add value="index.htm" />
                <add value="index.html" />
                <add value="index.php" />
                <add value="index.asp" />
                <add value="default.asp" />
                <add value="index.aspx" />
                <add value="default.aspx" />
            </files>
        </defaultDocument>
        <staticContent>
            <remove fileExtension=".json" />
            <mimeMap fileExtension=".json" mimeType="application/json" />
            <clientCache />
        </staticContent>
        <handlers>
            <add name="PHP-FastCGI8.2" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\php8.2\php-cgi_2.exe" resourceType="File" />
        </handlers>
    </system.webServer>
</configuration>